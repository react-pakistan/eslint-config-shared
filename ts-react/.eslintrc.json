{
  "extends": [
    "airbnb",
    "../common-ts/.eslintrc.json"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [2, { "extensions": [" .jsx", ".tsx"] }],
    "sort-imports": ["error", {
      "ignoreCase": false,
      "ignoreDeclarationSort": false,
      "ignoreMemberSort": false,
      "memberSyntaxSortOrder": ["none", "all", "multiple", "single"],
      "allowSeparatedGroups": false
    }]
  }
}
